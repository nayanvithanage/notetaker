{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Host=localhost;Database=notetaker;Username=postgres;Password=YOUR_POSTGRES_PASSWORD"
  },
  "App": {
    "BaseUrl": "http://localhost:5135",
    "FrontendUrl": "http://localhost:4200"
  },
  "JWT": {
    "Issuer": "NotetakerAPI",
    "Audience": "NotetakerUsers",
    "SigningKey": "YOUR_SUPER_SECRET_JWT_KEY_AT_LEAST_32_CHARACTERS_LONG",
    "ExpirationMinutes": 60,
    "RefreshExpirationDays": 7
  },
  "Google": {
    "ClientId": "YOUR_GOOGLE_CLIENT_ID",
    "ClientSecret": "YOUR_GOOGLE_CLIENT_SECRET",
    "RedirectUri": "http://localhost:5135/api/auth/google/callback"
  },
  "RecallAi": {
    "ApiKey": "YOUR_RECALL_AI_API_KEY",
    "BaseUrl": "https://api.recall.ai/api/v1"
  },
  "LinkedIn": {
    "ClientId": "YOUR_LINKEDIN_CLIENT_ID",
    "ClientSecret": "YOUR_LINKEDIN_CLIENT_SECRET",
    "RedirectUri": "http://localhost:5135/api/auth/social/callback"
  },
  "Facebook": {
    "AppId": "YOUR_FACEBOOK_APP_ID",
    "AppSecret": "YOUR_FACEBOOK_APP_SECRET",
    "RedirectUri": "http://localhost:5135/api/auth/social/callback"
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY",
    "Model": "gpt-4",
    "BaseUrl": "https://api.openai.com/v1"
  },
  "Bot": {
    "LeadMinutes": 5
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/notetaker-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
